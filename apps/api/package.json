{
    "name": "api",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "nest build",
        "dev": "pnpm run start:dev",
        "start": "NODE_ENV=production nest start -b swc --webpack -e 'node --experimental-require-module'",
        "start:dev": "NODE_ENV=development nest start -b swc --webpack --watch -e 'node --experimental-require-module'",
        "start:debug": "NEST_DEBUG='true' NODE_ENV=development -b swc nest start --webpack -e 'node --experimental-require-module' --debug --watch",
        "start:prod": "nest build && NODE_ENV=production node --experimental-require-module dist/main",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --passWithNoTests --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "format": "biome format . --write",
        "lint": "biome check . --write"
    },
    "dependencies": {
        "@liaoliaots/nestjs-redis": "catalog:",
